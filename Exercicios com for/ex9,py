# Com while
i = 1
while i <= 10:
    print(f"Tabuada do {i}:")
    j = 1
    while j <= 10:
        print(f"{i} * {j} = {i*j}")
        j += 1
    i += 1
    print()

# Com for
for i in range(1,11):
    print(f"Tabuada do {i}:")
    for j in range(1,11):
        print(f"{i} * {j} = {i*j}")
    print()